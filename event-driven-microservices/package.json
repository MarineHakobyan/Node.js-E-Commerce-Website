{
  "name": "event-driven-microservices",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/app.ts",
    "dev": "nodemon --watch src/ --exec npm start",
    "build": "tsc",
    "format": "prettier --write ./src/**/*.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm migration:generate -c migration -n",
    "migration:create": "npm run typeorm migration:create -c migration",
    "migration:run": "npm run typeorm migration:run -c migration",
    "migration:revert": "npm run typeorm migration:revert -c migration"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.21",
    "express": "^4.18.3",
    "pg": "^8.11.3",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
