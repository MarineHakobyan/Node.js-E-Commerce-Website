{
  "name": "auth-ms",
  "version": "1.0.0",
  "description": "",
  "module": "commonjs",
  "sourcemap": true,
  "strict": true,
  "emitDecoratorMetadata": true,
  "experimentalDecorators": true,
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/app.ts",
    "dev": "npm run build && nodemon --watch src/ --exec npm start",
    "build": "tsc",
    "format": "prettier --write ./src/**/*.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -c migration -n",
    "migration:create": "npm run typeorm -- migration:create -c migration -n",
    "migration:run": "npm run typeorm -- migration:run -c migration",
    "migration:revert": "npm run typeorm -- migration:revert -c migration",
    "seed:generate": "npm run typeorm -- migration:generate -c seed -n ",
    "seed:create": "npm run typeorm -- migration:create -c seed -n ",
    "seed:run": "npm run typeorm -- migration:run -c seed",
    "seed:revert": "npm run typeorm -- migration:revert -c seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-async-errors": "^3.1.1",
    "joi": "^17.12.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.2.41",
    "typeorm-naming-strategies": "^2.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/uuid": "^9.0.8",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2"
  }
}
