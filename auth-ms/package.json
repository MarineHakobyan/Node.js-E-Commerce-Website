{
  "name": "auth-ms",
  "version": "1.0.0",
  "description": "",
  "module": "commonjs",
  "sourcemap": true,
  "strict": true,
  "emitDecoratorMetadata": true,
  "experimentalDecorators": true,
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/app.ts",
    "dev": "nodemon --watch src/ --exec npm start",
    "build": "tsc",
    "format": "prettier --write ./src/**/*.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.ts",
    "migration:generate": "typeorm migration:create -n",
    "migration:create": "ts-node src/script/create-db.ts",
    "migration:migrate": "npm run typeorm:cli migration:run",
    "migration:revert": "npm run typeorm:cli migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-async-errors": "^3.1.1",
    "joi": "^17.12.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.2.45",
    "typeorm-naming-strategies": "^2.0.0",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2"
  }
}
